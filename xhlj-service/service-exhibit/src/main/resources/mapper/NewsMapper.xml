<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.swyxl.exhibit.mapper.NewsMapper">

    <sql id="columns">
        id, title, introduction, text, image, source, is_deleted, create_time, update_time
    </sql>

    <select id="selectAll" resultType="com.swyxl.model.entity.service.exhibit.News">
        select <include refid="columns"/> from news where is_deleted = 0
    </select>
</mapper>